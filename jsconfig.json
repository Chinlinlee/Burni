{
    "compilerOptions": {
        "module": "CommonJS",
        "paths": {
            "@root/*": ["./*"],
            "@models/*": ["./models/*"],
            "@mongodb/*": ["./models/mongodb/*"],
            "@FhirApiService/*": ["./api/FhirApiService/*"],
            "@api/*": ["./api/*"]
        }
    },
    "exclude": [
        "node_modules", "**/node_modules/*"
    ]
}